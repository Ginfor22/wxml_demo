.weui-cells:before{
  top:0;
  border-top:1rpx solid white;
  }
.weui-cell{
  line-height: 3.5rem;
}
.weui-cells:after{
  bottom:0;border-bottom:1rpx solid white
}
.btn {
  display: flex; 
  width: 100%;
  height: 130rpx;
  }
.btn .weui-btn{
  width: 70%;
  margin-top:20px;
  background-color: #00847f;
  display: flex;
  flex-direction: column;
  vertical-align:middle;
  }
  .btn text{
    font-size: 35rpx;
    text-align: center;
    line-height: 75rpx;
  }
.btn-hover{
    position: relative;
    top: 3rpx;
    box-shadow:0px 0px 8px #999 inset; 
  }
